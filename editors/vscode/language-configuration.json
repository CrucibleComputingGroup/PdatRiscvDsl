{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" }
  ],
  "surroundingPairs": [
    { "open": "{", "close": "}" }
  ],
  "folding": {
    "markers": {
      "start": "^\\s*#\\s*region\\b",
      "end": "^\\s*#\\s*endregion\\b"
    }
  }
}
